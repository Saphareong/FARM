@model BusinessObj.Models.UserClass

@{
    ViewData["Title"] = "Create";
}

<h2>Conform</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Conform">
            <div class="form-group">
                <label asp-for="SubjectID" class="control-label"></label>
                <input name="SubjectID" class="form-control" value="@ViewBag.SubjectV2" readonly/>
            </div>
            <div class="form-group">
                <label asp-for="ClassID" class="control-label"></label>
                <select name="ClassID" class ="form-control" asp-items="ViewBag.Class"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Conform" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="Create">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
